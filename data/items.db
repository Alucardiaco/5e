:3},"type":"consumable","data":{"description":{"value":"<div>\n\t\t\t<div><i>Potion, minor tier, common</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">50 gp, 8 oz.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>You regain [[/r 2d4 + 2]] hit points when you drink this potion. The potion's red liquid glimmers when agitated.</p></div>","chat":"","unidentified":""},"source":"DMG","quantity":3,"weight":0.5,"price":50,"attuned":false,"equipped":true,"rarity":"Common","identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":0,"units":""},"target":{"value":1,"width":null,"units":"","type":"creature"},"range":{"value":null,"long":null,"units":"touch"},"uses":{"value":1,"max":1,"per":"charges","autoDestroy":true,"autoUse":true},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"heal","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[["2d4 + 2","healing"]],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"consumableType":"potion","attributes":{"spelldc":10}},"folder":"kVf4ry2A3cfFTXZi","sort":200000,"flags":{"obsidian":{"unlimited":false,"subtype":"potion","effects":[{"name":"","uuid":"8d5a56a5-b6ef-4892-8819-df2906c74656","components":[{"type":"target","target":"individual","count":1,"area":"cone","distance":0,"uuid":"65ca2719-7127-4f68-b042-74c211bb7b86"},{"type":"resource","name":"Charges","recharge":{"time":"long","calc":"all","ndice":0,"die":2,"bonus":0,"roll":null},"bonus":0,"operator":"plus","key":"abl","min":0,"calc":"fixed","fixed":1,"uuid":"6c6085f4-379a-4638-b137-e79207cc06c4","remaining":0},{"type":"damage","calc":"formula","ndice":2,"die":4,"ability":"","bonus":2,"damage":"hlg","versatile":false,"uuid":"31bba81d-5413-4327-a67a-a805f8cd6825"}]}],"version":10},"betterRolls5e":{"quickDamage":{"context":{"0":""},"value":{"0":true},"altValue":{"0":true}}}},"img